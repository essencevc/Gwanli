{
  "name": "gwanli-core",
  "version": "0.2.0",
  "description": "Core library for Notion workspace management and search",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/lib/init.sql dist/lib/",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "keywords": [
    "notion",
    "search",
    "indexing",
    "markdown",
    "workspace"
  ],
  "author": "Ivan Leo & Timothy Chen",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "notion-to-md": "^3.1.9",
    "better-sqlite3": "^11.5.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.0"
  },
  "sideEffects": false
}
